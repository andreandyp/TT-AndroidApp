<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/navigation_graph"
        app:startDestination="@id/bienvenidaConceptosFragment">
    <fragment
            android:id="@+id/bienvenidaConceptosFragment"
            android:name="com.apptt.axdecor.fragments.BienvenidaConceptosFragment"
            android:label="fragment_bienvenida_conceptos"
            tools:layout="@layout/fragment_bienvenida_conceptos" >
        <action
                android:id="@+id/action_bienvenidaConceptosFragment_to_conceptosFragment"
                app:destination="@id/conceptosFragment"
                app:enterAnim="@anim/fragment_fade_enter"
                app:exitAnim="@anim/fragment_fade_exit"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"
                app:popUpTo="@+id/bienvenidaConceptosFragment" />
        <action
                android:id="@+id/action_bienvenidaConceptosFragment_to_proveedoresFragment"
                app:destination="@id/proveedoresFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/conceptosFragment"
            android:name="com.apptt.axdecor.fragments.ConceptosFragment"
            android:label="ConceptosFragment"
            tools:layout="@layout/fragment_conceptos">
        <action
                android:id="@+id/action_conceptosFragment_to_proveedoresFragment"
                app:destination="@id/proveedoresFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/proveedoresFragment"
            android:name="com.apptt.axdecor.fragments.ProveedoresFragment"
            android:label="fragment_proveedores"
            tools:layout="@layout/fragment_proveedores" />
</navigation>